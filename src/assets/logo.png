  </defs>
  <!-- rounded background -->
  <rect width="128" height="128" rx="24" fill="url(#mn-grad)" />
  <!-- MN monogram -->
  <g fill="#ffffff" font-family="Segoe UI, Roboto, Arial, sans-serif" font-weight="700" text-anchor="middle">
    <text x="50%" y="58%" font-size="56">MN</text>
  </g>
